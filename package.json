{
  "name": "healthtracker",
  "version": "1.0.0",
  "description": "A Fitness and Nutrition Tracking App",
  "engines": {
    "node": ">=20.0.0"
  },
  "main": "server.js",
  "scripts": {
    "client": "node scripts/start-client.js",
