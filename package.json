    "healthtracker": "^0.1.1",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.22.2",
    "mongoose": "^8.9.2",
    "morgan": "^1.9.1",
    "nodemon": "^3.1.9",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.0"
  },
  "root": "build/",
  "clean_urls": false,
  "routes": {
    "/**": "index.html"
